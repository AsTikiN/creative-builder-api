generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Creative {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  title        String
  description  String
  image        String
  isSaved      Boolean  @default(false)
  keywords     String[]
  alt          String
  createdAt    DateTime @default(now())
}
